{
  "private": true,
  "workspaces": [
    "examples/*",
    "packages/*"
  ],
  "scripts": {
    "build": "yarn run build:ingest && yarn run build:http",
    "build:ingest": "yarn --cwd packages/ingest build",
    "build:http": "yarn --cwd examples/with-http build",
    "dev:http": "yarn --cwd examples/with-http dev"
  },
  "devDependencies": {
    "npm-run-all": "4.1.5"
  }
}